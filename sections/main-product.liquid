          let tempFormData = {}
          formData.forEach((value, key) => {
            tempFormData = {...tempFormData, [key]: value }
          });
          tempFormData = {items:[tempFormData]}
          
           console.log(tempFormData)
 
           fetch(window.Shopify.routes.root + 'cart/add.js', {
               method: 'POST',
               headers: {
                 'Content-Type': 'application/json'
